<tr>
  <td><%= test.category.title %></td>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.author.email %></td>
  <td><%= questions_count(test) %></td>
  <td><%= link_to t('.show'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('.delete'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('.confirm') } %>
  </td>
  <td><%= button_to t('.start'), start_test_path(test) %></td>
</tr>
